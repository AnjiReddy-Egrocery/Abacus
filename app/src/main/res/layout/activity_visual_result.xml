<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_margin="14dp">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:src="@drawable/big_logo_total"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"/>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="1dp"
                    android:background="@drawable/item_background_images"
                    android:orientation="vertical">
                    <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">
                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:weightSum="3"
                            android:background="@drawable/cell_shape">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:textSize="14dp"
                                android:fontFamily="@font/regular"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textStyle="bold"
                                android:textColor="#000"
                                android:text="Student Name"/>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:textSize="14dp"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:fontFamily="@font/regular"
                                android:textStyle="bold"
                                android:textColor="#000"
                                android:text=" : "/>

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:id="@+id/txt_student"
                                android:textSize="14dp"
                                android:fontFamily="@font/regular"
                                android:layout_weight="1"
                                android:textColor="#000"
                                android:gravity="center"
                                android:text=""/>

                        </TableRow>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@android:color/darker_gray" />
                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:weightSum="3"
                            android:background="@drawable/cell_shape">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:textSize="14dp"
                                android:fontFamily="@font/regular"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textStyle="bold"
                                android:textColor="#000"
                                android:text="submited On"/>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:textSize="14dp"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:fontFamily="@font/regular"
                                android:textStyle="bold"
                                android:textColor="#000"
                                android:text=" : "/>

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:id="@+id/txt_submitted_on"
                                android:textSize="14dp"
                                android:fontFamily="@font/regular"
                                android:layout_weight="1"
                                android:textColor="#000"
                                android:gravity="center"
                                android:text=""/>

                        </TableRow>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@android:color/darker_gray" />
                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:weightSum="3"
                            android:background="@drawable/cell_shape">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:textSize="14dp"
                                android:fontFamily="@font/regular"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:textStyle="bold"
                                android:textColor="#000"
                                android:text="Total Questions"/>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:textSize="14dp"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:fontFamily="@font/regular"
                                android:textStyle="bold"
                                android:textColor="#000"
                                android:text=" : "/>

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:id="@+id/txt_questions"
                                android:textSize="14dp"
                                android:fontFamily="@font/regular"
                                android:layout_weight="1"
                                android:textColor="#000"
                                android:gravity="center"
                                android:text=""/>

                        </TableRow>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@android:color/darker_gray" />
                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:weightSum="3"
                            android:background="@drawable/cell_shape">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_weight="1"
                                android:textSize="14dp"
                                android:textStyle="bold"
                                android:gravity="center"
                                android:textColor="#000"
                                android:fontFamily="@font/regular"
                                android:text="Attemted Questions"/>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:textSize="14dp"
                                android:textStyle="bold"
                                android:gravity="center"
                                android:textColor="#000"
                                android:fontFamily="@font/regular"
                                android:text=":"/>

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/regular"
                                android:textSize="14dp"
                                android:id="@+id/txt_attemted_question"
                                android:gravity="center"
                                android:padding="5dp"
                                android:textColor="#000"
                                android:layout_weight="1"
                                android:text=""/>
                        </TableRow>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@android:color/darker_gray" />




                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:weightSum="3"
                            android:background="@drawable/cell_shape">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:textSize="14dp"
                                android:fontFamily="@font/regular"
                                android:layout_weight="1"
                                android:textStyle="bold"
                                android:textColor="#000"
                                android:gravity="center"
                                android:text="Not Attemted Questions"/>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:textSize="14dp"
                                android:fontFamily="@font/regular"
                                android:layout_weight="1"
                                android:textStyle="bold"
                                android:textColor="#000"
                                android:gravity="center"
                                android:text=":"/>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:fontFamily="@font/regular"
                                android:gravity="center"
                                android:id="@+id/txt_not_questions"
                                android:textSize="14dp"
                                android:textColor="#000"
                                android:layout_weight="1"
                                android:text=""/>
                        </TableRow>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@android:color/darker_gray" />
                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:weightSum="3"
                            android:background="@drawable/cell_shape">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:textSize="14dp"
                                android:layout_weight="1"
                                android:textStyle="bold"
                                android:gravity="center"
                                android:fontFamily="@font/regular"
                                android:textColor="#000"
                                android:text="Correct Answers"/>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:textSize="14dp"
                                android:layout_weight="1"
                                android:textStyle="bold"
                                android:gravity="center"
                                android:fontFamily="@font/regular"
                                android:textColor="#000"
                                android:text=":"/>

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:fontFamily="@font/regular"
                                android:textSize="14dp"
                                android:gravity="center"
                                android:text=""
                                android:id="@+id/txt_correct_answer"
                                android:textColor="#000"
                                android:layout_weight="1"
                                android:textAppearance="?android:attr/textAppearanceMedium">
                            </TextView>
                        </TableRow>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@android:color/darker_gray" />
                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:weightSum="3"
                            android:background="@drawable/cell_shape">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:textSize="14dp"
                                android:gravity="center"
                                android:fontFamily="@font/regular"
                                android:textStyle="bold"
                                android:layout_weight="1"
                                android:textColor="#000"
                                android:text="Wrong Answers"/>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:textSize="14dp"
                                android:gravity="center"
                                android:fontFamily="@font/regular"
                                android:textStyle="bold"
                                android:layout_weight="1"
                                android:textColor="#000"
                                android:text=":"/>

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:text=""
                                android:id="@+id/txt_wrong_answer"
                                android:gravity="center"
                                android:fontFamily="@font/regular"
                                android:textSize="14dp"
                                android:textColor="#000"
                                android:layout_weight="1"/>
                        </TableRow>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@android:color/darker_gray" />
                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:weightSum="3"
                            android:background="@drawable/cell_shape">
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:textSize="14dp"
                                android:gravity="center"
                                android:fontFamily="@font/regular"
                                android:textStyle="bold"
                                android:layout_weight="1"
                                android:textColor="#000"
                                android:text="Time Interval"/>
                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:textSize="14dp"
                                android:gravity="center"
                                android:fontFamily="@font/regular"
                                android:textStyle="bold"
                                android:layout_weight="1"
                                android:textColor="#000"
                                android:text=":"/>

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:text=""
                                android:id="@+id/txt_time_interval"
                                android:gravity="center"
                                android:fontFamily="@font/regular"
                                android:textSize="14dp"
                                android:textColor="#000"
                                android:layout_weight="1"/>
                        </TableRow>


                    </TableLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="14dp"
                    android:background="@drawable/item_background_images"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cell_shape"
                        android:orientation="horizontal">
                        <TableLayout
                            android:id="@+id/tablelayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:stretchColumns="1">

                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:weightSum="3">

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/regular"
                                    android:gravity="center"
                                    android:textSize="16dp"
                                    android:layout_weight="1"
                                    android:textColor="#000"
                                    android:text="Question"
                                    android:textStyle="bold" />



                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:textColor="#000"
                                    android:textSize="16dp"
                                    android:fontFamily="@font/regular"
                                    android:text="Answer"
                                    android:textStyle="bold" />


                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:textSize="16dp"
                                    android:fontFamily="@font/regular"
                                    android:text="Given"
                                    android:textColor="#000"
                                    android:textStyle="bold" />


                            </TableRow>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@android:color/darker_gray" />
                        </TableLayout>

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <ListView
                android:id="@+id/listView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:divider="@android:color/darker_gray"
                android:dividerHeight="1dp"
                android:padding="16dp" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>